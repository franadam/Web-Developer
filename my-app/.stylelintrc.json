{
    "env": {
        "browser": true
    },
    "extends": "stylelint-config-recommended",
    "plugins": [
      "stylelint-scss",
      "stylelint-order"
    ],
    "rules": {
      "number-leading-zero": null,
      "order/order": [
          "custom-properties",
          "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
}